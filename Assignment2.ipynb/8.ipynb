    "import time\n",
    "\n",
    "def file_operations():\n",
    "    os.makedirs(\"test_folder\")\n",
    "    time.sleep(3)\n",
    "    os.rmdir(\"test_folder\")\n",
    "\n",
    "# Example Usage:\n",
    "file_operations()  # Creates \"test_folder\", pauses for 3 seconds, and deletes it."
