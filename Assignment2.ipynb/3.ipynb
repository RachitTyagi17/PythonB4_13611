   "source": [
    "def sum_numbers(*args, **kwargs):\n",
    "    args_sum = sum(arg for arg in args if isinstance(arg, (int, float)))\n",
    "    return args_sum, kwargs\n",
    "\n",
    "# Example Usage:\n",
    "print(sum_numbers(1, 2, 3))  # Output: (6, {})\n",
    "print(sum_numbers(1, 2, x=4, y=5))  # Output: (3, {'x': 4, 'y': 5})"
   ]
